<Window x:Class="WpfApp2.SignupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Signup" Height="400" Width="400">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Username:"/>
            <TextBox x:Name="UsernameBox" Margin="0,5"/>
            <TextBlock Text="Password:"/>
            <PasswordBox x:Name="PasswordBox" Margin="0,5"/>
            <TextBlock Text="Email:"/>
            <TextBox x:Name="EmailBox" Margin="0,5"/>
            <Button Content="Signup" Margin="0,10" Click="Signup_Click"/>
            <Button Content="Back to Login" Click="BackToLogin_Click"/>
        </StackPanel>
    </Grid>
</Window>